{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
    "types": ["@remix-run/node", "vite/client"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    }
  }
}
